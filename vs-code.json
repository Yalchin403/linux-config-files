{
    "explorer.confirmDelete": false,
    "workbench.colorTheme": "NarutoDark",


    "kite.showWelcomeNotificationOnStartup": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.languageServer": "Microsoft",
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.commandLineModeKeyBindingsNonRecursive": [
    ],
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "typescript.tsdk": "",

    "vim.handleKeys": {
    "<C-c>": false,
    "<C-a>": false,
    // VS Code search in file
    "<C-f>": false,
    // VS Code new marker @ next occurence
    "<C-d>": false,
    // Copy
    // Cut
    "<C-x>": false,
    // Paste
    "<C-v>": false,
    "C-s": false,
},
"vim.insertModeKeyBindings": [
        // In insert mode jj to escape
        {
        "before": ["j", "j"],
        "after": ["<Esc>"]
        }
    ],
"vim.visualModeKeyBindings": [
        // In visual mode i goes to insert mode
        {
        "before": ["i"],
        "after": ["<Esc>", "i"]
        }
    ],
    // "editor.lineNumbers": "off",
    "ponicode.token": "d949df55-bfbf-4755-8b8b-88210556651d",
    "workbench.iconTheme": "material-icon-theme",
    "window.zoomLevel": 1,
}
